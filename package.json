{
  "name": "modus-cli",
  "version": "1.0.0",
  "description": "Expandable CLI tool for Modus Create projects",
  "main": "index.js",
  "scripts": {
    "prettify": "prettier --write \"**/*.js\"",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ModusCreateOrg/modus-cli.git"
  },
  "keywords": [
    "Modus",
    "Create"
  ],
  "author": "Mike Schwartz <mike@moduscreate.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ModusCreateOrg/modus-cli/issues"
  },
  "homepage": "https://github.com/ModusCreateOrg/modus-cli#readme",
  "dependencies": {
    "@octokit/rest": "^15.6.1",
    "commander": "^2.15.1"
  },
  "devDependencies": {
    "husky": "0.14.3",
    "lint-staged": "7.1.2",
    "prettier": "1.13.2"
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run prettify",
      "git add"
    ]
  }
}
